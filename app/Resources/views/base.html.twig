<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}ArianEspace  - Opus{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">

        {% stylesheets filter='less'
        'bundles/formgenerator/css/footer.less' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css"/>
        {% endstylesheets %}
    {% endblock %}
    {% block javascripts %}
        <script src="http://code.jquery.com/jquery-latest.js"></script>
        <script src="{{ asset('js/bootstrap.js') }}"></script>
        <script src="{{ asset('js/jquery.js') }}"></script>
        {% javascripts '@data_tables' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
</head>
<body>

{% block mainmenu %} {{ knp_menu_render('FormGeneratorBundle:Builder:mainMenu') }} {% endblock %}
<div class="container theme-showcase content" style="margin-top:60px" role="main">
    {% block body %}
    {% endblock %}
</div>


{% block footer %}
    <footer class="footer">
        <div class="container">
            <p class="text-muted">Place sticky footer content here.</p>
        </div>
    </footer>
{% endblock %}
</body>
</html>
