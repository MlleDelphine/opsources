{% extends 'AppBundle::layout.html.twig' %}
{% block body %}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Tableau de bord</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-success">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa {{ icon }} fa-fw"></i> {{ title }} </h3>
            </div>
            <div class="panel-body tabbable-custom">
                {{ addDataTable(dataTable, {
                    'path':          path( (routeName), {'tableName':'meetDataTable'}),
                    'id':            'meetDataTable',
                    'bLengthChange': 1,
                    'bInfo':         1,
                    'bFilter':       1,
                    'language': {
                        'url': (asset('vendor/datatables-plugins/i18n/French.lang'))
                    }

                }) }}
            </div>
        </div>
    </div>

    {% endblock %}

    {% block javascripts %}
        {{ parent() }}

    {%  endblock %}



